{% extends 'base.html.twig' %}

{% block title %}Calls{% endblock %}

{% block body %}
    <h1>Calls</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ call.id }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ call.status }}</td>
            </tr>
            <tr>
                <th>Floor_from</th>
                <td>{{ call.floorFrom }}</td>
            </tr>
            <tr>
                <th>Floor_to</th>
                <td>{{ call.floorTo }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('calls_index') }}">back to list</a>

    <a href="{{ path('calls_edit', {'id': call.id}) }}">edit</a>

    {{ include('calls/_delete_form.html.twig') }}
{% endblock %}
